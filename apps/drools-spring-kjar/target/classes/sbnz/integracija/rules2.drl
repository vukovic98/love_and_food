package com.ftn.uns.ac.rs.love_and_food;

import com.ftn.uns.ac.rs.love_and_food.facts.Item;

rule "Classify Item - High Range"
    when
        $i: Item( cost > 300, category == Category.NA )
    then
    	System.out.println("High range rule");
        $i.setCategory(Item.Category.HIGH_RANGE);
        update($i);
end